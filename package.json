{
  "name": "web-components",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist",
    "src/components/"
  ],
  "main": "./dist/web-components.umd.cjs",
  "module": "./dist/web-components.js",
  "exports": {
    ".": {
      "import": "./dist/web-components.js",
      "require": "./dist/web-components.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "rollup": "rollup -c",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "storybook-docs": "storybook --docs --no-manager-cache",
    "build-storybook": "storybook build -o ./dist",
    "build-storybook-docs": "storybook build --docs"
  },
  "peerDependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-themes": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/manager-api": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@storybook/vue3": "^7.6.17",
    "@storybook/vue3-vite": "^7.6.17",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.17",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.3",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "vite-svg-loader": "^5.1.0"
  }
}
