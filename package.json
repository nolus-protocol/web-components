  "private": true,
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "rollup": "rollup -c",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
